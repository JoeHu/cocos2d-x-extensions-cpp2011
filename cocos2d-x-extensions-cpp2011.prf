
isEmpty(COCOS2D_VERSION_MAJOR):COCOS2D_VERSION_MAJOR=3

equals(COCOS2D_VERSION_MAJOR, 3) {
    INCLUDEPATH *= $$PWD/cocos2dx_v3/async
    INCLUDEPATH *= $$PWD/cocos2dx_v3/utils
